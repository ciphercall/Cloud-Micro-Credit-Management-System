                                                                                                                                                                            sumexpense = Convert.ToDecimal(sumexpense + m.Debitamt);
                                                                                                                                                                        }

                                                                                                                                                                    }
                                                                                                                                                                }
                                                                                                                                                                if (EStatus == "yes" && sumexpense != 0)
                                                                                                                                                                {
                                                                                                                                                                    <tr style="outline: thin solid">
                                                                                                                                                                        <td colspan="3" style="padding-left: 8px">@ffdtE</td>
                                                                                                                                                                        <td colspan="3" align="left">@transNoE</td>
                                                                                                                                                                        <td colspan="4" align="left">@debitExpense</td>
                                                                                                                                                                        <td colspan="4" align="left">@creditExpense</td>
                                                                                                                                                                        <td colspan="3" align="right">@remarksExpense</td>
                                                                                                                                                                        @{
                                                                                                                                                                            string convertamount8 = Convert.ToString(sumexpense);
                                                                                                                                                                            string Sumexpense = CommainAmount.AmountwithComma(convertamount8);
                                                                                                                                                                        }
                                                                                                                                                                        <td colspan="3" align="right">@Sumexpense</td>
                                                                                                                                                                    </tr>
                                                                                                                                                                }
                                                                                                                                                                totalExpense = Convert.ToDecimal(totalExpense + sumexpense);
                                                                                                                                                            }
                                                                                                                                                            string convertamount10 = Convert.ToString(totalExpense);
                                                                                                                                                            string tExpense = CommainAmount.AmountwithComma(convertamount10);

                                                                                                                                                            if (totalExpense != 0)
                                                                                                                                                            {
                                                                                                                                                                <tr style="outline: thin solid">
                                                                                                                                                                    <td colspan="17" style="font-weight: bold" align="right">Total Expenditure : </td>
                                                                                                                                                                    <td colspan="3" style="font-weight: bold" align="right">@tExpense</td>
                                                                                                                                                                </tr>
                                                                                                                                                            }
                                                                                                                                                        }
                                                                                    }

            </tbody>
        </table>
